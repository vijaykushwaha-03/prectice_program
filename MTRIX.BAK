#include<stdio.h>
#include<conio.h>

void main()
{
	int a[10][10],i,j,r,c;

	clrscr();

	//read

	printf("\n\n\t Enter row and column =>");
	scanf("%d,%d",&r,&c);

	for(i=0; i<r; i++)
	{
		for(j=0; j<c; j++)
		{
			printf("\n\n\t Enter a[%d] [%d] =",i,j);
			scanf("%d",&a[i][j]);
		}
	}

	// write

	for(i=0; i<r; i++)
	{
		for(j=0; j<c; j++)
		{
			printf(" %d ",a[i][j]);

		}
		printf("\n\n");
	}
	getch();
}